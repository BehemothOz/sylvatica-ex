{
    "name": "sylvatica",
    "displayName": "sylvatica",
    "description": "Extension for VS Code",
    "version": "1.0.0",
    "publisher": "WoodenHands",
    "repository": "https://github.com/BehemothOz/sylvatica-ex",
    "engines": {
        "vscode": "^1.89.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "extension.refresh",
                "title": "Refresh"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.helloWorld"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./",
        "jest:test": "jest"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^18",
        "@types/semver": "^7.5.8",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^7.14.0",
        "@typescript-eslint/parser": "^7.14.0",
        "eslint": "^8.26.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.4",
        "typescript": "^5.5.2"
    },
    "dependencies": {
        "semver": "^7.6.3"
    }
}
